    "@apollo/client": "^3.4.15",
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@graphql-tools/schema": "^8.2.0",
    "@material-ui/core": "^4.12.3",
