    "react-dom": "^18.2.0",
    "react-virtualized": "^9.22.3",
    "rimraf": "^4.1.2",
    "semantic-release": "^20.1.0",
    "sharp": "^0.31.3",
    "ts-jest": "^29.0.5",
    "turndown": "^7.1.1",
    "turndown-plugin-gfm": "^1.0.2",
    "typescript": "^4.9.5",
    "typescript-eslint": "^0.0.1-alpha.0",
    "url-parse": "^1.5.10",
    "yup": "^0.32.11"
