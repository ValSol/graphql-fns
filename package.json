{
  "name": "graphql-fns",
  "version": "0.1.2-beta.1102",
  "description": "Useful functions to work with graphql",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsup",
    "prepack": "yarn build",
    "commit": "git-cz",
    "test:once": "jest --config=\"./configs/jest.config.js\" --runInBand --verbose false",
    "test:mongodb": "jest --config=\"./configs/jest.config.mongodb.js\" --runInBand --verbose false --detectOpenHandles",
    "test": "jest  --config=\"./configs/jest.config.js\" --watch",
    "semantic-release": "semantic-release",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ValSol/graphql-fns.git"
  },
  "keywords": [
    "graphql",
    "utils"
  ],
  "author": "Valentyn Soloviov <v.solovyov@intellect.ua>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ValSol/graphql-fns/issues"
  },
  "homepage": "https://github.com/ValSol/graphql-fns#readme",
  "dependencies": {
    "deep-object-diff": "^1.1.9",
    "graphql-parse-resolve-info": "^4.13.0",
    "graphql-scalars": "1.23.0",
    "lodash.merge": "^4.6.2",
    "mingo": "^6.5.2",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@graphql-tools/schema": "^10.0.16",
    "@graphql-yoga/subscription": "^5.0.5",
    "@shelf/jest-mongodb": "^4.3.2",
    "@types/jest": "^29.5.14",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "20.9.0",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^16.10.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-node": "^29.7.0",
    "mkdirp": "^3.0.1",
    "mongoose": "^8.16.2",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.1",
    "ts-jest": "^29.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.35.1"
  },
  "peerDependencies": {
    "@graphql-yoga/subscription": "5.x",
    "fast-deep-equal": "3.x",
    "graphql": "16.x",
    "mongoose": "8.x"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "yarn test:once"
    }
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
