{
  "name": "graphql-fns",
  "version": "0.1.2-beta.779",
  "description": "Useful functions to work with graphql",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn build:clean && yarn build:lib && yarn build:flow",
    "build:clean": "rimraf lib",
    "build:lib": "BABEL_ENV=production && babel src --out-dir lib --ignore \"src/**/*.mtest.js\",\"src/**/*.test.js\",\"src/**/*.auxiliary.js\"",
    "build:flow": "flow-copy-source src lib --ignore \"**/*.mtest.js\" --ignore \"**/*.test.js\"",
    "commit": "git-cz",
    "flow": "flow check",
    "test:once": "jest --runInBand --verbose false",
    "test:mongodb": "jest --config=jest.config.mongodb.js --runInBand --verbose false --detectOpenHandles",
    "test": "jest --watch",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ValSol/graphql-fns.git"
  },
  "keywords": [
    "graphql",
    "utils"
  ],
  "author": "Valentyn Soloviov <v.solovyov@intellect.ua>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ValSol/graphql-fns/issues"
  },
  "homepage": "https://github.com/ValSol/graphql-fns#readme",
  "dependencies": {
    "csv-parse": "4.14.2",
    "csv-stringify": "^6.2.3",
    "get-stream": "^6.0.1",
    "graphql-scalars": "^1.20.1",
    "graphql-upload": "13.0.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "mingo": "^6.2.7",
    "pluralize": "^8.0.0",
    "spark-md5": "^3.0.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.7.5",
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@google-cloud/translate": "^7.0.5",
    "@graphql-tools/schema": "^9.0.13",
    "@shelf/jest-mongodb": "^4.1.6",
    "babel-jest": "^29.4.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-minify": "^0.5.2",
    "bitwise-array": "^0.1.14",
    "commitizen": "^4.3.0",
