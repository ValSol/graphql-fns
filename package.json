{
  "name": "graphql-fns",
  "version": "0.1.2-beta.282",
  "description": "Useful functions to work with graphql",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn build:clean && yarn build:lib && yarn build:flow",
    "build:clean": "rimraf lib",
    "build:lib": "BABEL_ENV=production && babel src --out-dir lib --ignore \"src/**/*.mtest.js\",\"src/**/*.test.js\",\"src/**/*.auxiliary.js\"",
    "build:flow": "flow-copy-source src lib --ignore \"**/*.mtest.js\" --ignore \"**/*.test.js\"",
    "commit": "git-cz",
    "flow": "flow check",
    "test:once": "jest",
    "test:mongodb": "jest --config=jest.config.mongodb.js",
    "test": "jest --watch",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ValSol/graphql-fns.git"
  },
  "keywords": [
    "graphql",
    "utils"
  ],
  "author": "Valentyn Soloviov <v.solovyov@intellect.ua>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ValSol/graphql-fns/issues"
  },
  "homepage": "https://github.com/ValSol/graphql-fns#readme",
  "dependencies": {
    "@okgrow/graphql-scalars": "^0.4.5",
    "clsx": "^1.0.4",
    "fast-deep-equal": "^2.0.1",
    "lodash": "^4.17.14",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.0",
    "apollo-server": "^2.6.9",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-minify": "^0.5.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "flow-bin": "^0.102.0",
    "flow-copy-source": "^2.0.7",
    "formik": "^1.5.8",
    "formik-material-ui": "^0.0.19",
    "ghooks": "^2.0.4",
    "graphql": "^14.4.2",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tag": "^2.10.1",
    "jest": "^24.8.0",
    "jest-environment-node": "^24.8.0",
    "mongoose": "^5.6.4",
    "next": "^9.0.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "react-virtualized": "^9.21.1",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.18",
    "yup": "^0.27.0"
  },
  "peerDependencies": {
    "@material-ui/core": "4.x",
    "@material-ui/icons": "4.x",
    "@material-ui/styles": "4.x",
    "formik": "1.5.x",
    "formik-material-ui": "0.0.x",
    "graphql": "14.x",
    "graphql-subscriptions": "1.x",
    "graphql-tag": "2.x",
    "mongoose": "5.x",
    "next": "9.x",
    "react": "16.8.x",
    "react-apollo": "2.x",
    "react-dom": "16.8.x",
    "react-virtualized": "9.x",
    "yup": "0.27.x"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "yarn flow && yarn test:once && yarn test:mongodb"
    }
  }
}
