{
  "name": "graphql-fns",
  "version": "0.1.2-beta.671",
  "description": "Useful functions to work with graphql",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn build:clean && yarn build:lib && yarn build:flow",
    "build:clean": "rimraf lib",
    "build:lib": "BABEL_ENV=production && babel src --out-dir lib --ignore \"src/**/*.mtest.js\",\"src/**/*.test.js\",\"src/**/*.auxiliary.js\"",
    "build:flow": "flow-copy-source src lib --ignore \"**/*.mtest.js\" --ignore \"**/*.test.js\"",
    "commit": "git-cz",
    "flow": "flow check",
    "test:once": "jest --verbose false",
    "test:mongodb": "jest --config=jest.config.mongodb.js --runInBand --verbose false",
    "test": "jest --watch",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ValSol/graphql-fns.git"
  },
  "keywords": [
    "graphql",
    "utils"
  ],
  "author": "Valentyn Soloviov <v.solovyov@intellect.ua>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ValSol/graphql-fns/issues"
  },
  "homepage": "https://github.com/ValSol/graphql-fns#readme",
  "dependencies": {
    "bitwise-array": "^0.1.14",
    "clsx": "^1.1.1",
    "csv-parse": "4.14.2",
    "csv-stringify": "^6.0.5",
    "get-stream": "^6.0.1",
    "graphql-scalars": "^1.14.1",
    "graphql-upload": "^13.0.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "mingo": "^5.0.0",
    "pluralize": "^8.0.0",
    "spark-md5": "^3.0.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.5.6",
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@graphql-tools/schema": "^8.3.1",
    "babel-jest": "^27.4.5",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-minify": "^0.5.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.16.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "exceljs": "^4.3.0",
    "fast-deep-equal": "^3.1.3",
    "flow-bin": "0.141.0",
    "flow-copy-source": "^2.0.9",
    "formik": "^2.2.9",
    "ghooks": "^2.0.4",
    "graphql": "^16.2.0",
    "graphql-subscriptions": "^2.0.0",
    "jest": "^27.4.5",
    "jest-environment-node": "^27.4.4",
    "mongodb": "^4.2.2",
    "mongoose": "^6.1.5",
    "next": "^12.0.7",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-virtualized": "^9.22.3",
    "rimraf": "^3.0.2",
    "semantic-release": "^18.0.1",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "@apollo/client": "^3.x",
    "exceljs": "^4.x",
    "fast-deep-equal": "3.x",
    "formik": "2.x",
    "graphql": "16.x",
    "graphql-subscriptions": "2.x",
    "mongoose": "6.x",
    "next": "12.x",
    "react": "17.x",
    "react-dom": "17.x",
    "react-virtualized": "9.x",
    "yup": "0.32.x"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "yarn flow && yarn test:once && yarn test:mongodb"
    }
  }
}
