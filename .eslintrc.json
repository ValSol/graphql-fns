{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/button-has-type": false,
    "react/jsx-filename-extension": false,
    "react/jsx-one-expression-per-line": false
  }
}
